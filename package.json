{
	"name": "prisma-class-generator",
	"version": "0.1.2",
	"description": "Class generator from Prisma schema",
	"main": "dist/index.js",
	"license": "MIT",
	"keywords": [
		"prisma",
		"nestjs",
		"typescript",
		"generator"
	],
	"homepage": "https://github.com/kimjbstar/prisma-class-generator",
	"repository": {
		"type": "git",
		"url": "git://github.com/kimjbstar/prisma-class-generator.git"
	},
	"scripts": {
		"format": "prettier --write \"src/**/*.ts\"",
		"dev": "node -r ts-node/register -r tsconfig-paths/register src/index.ts"
	},
	"bin": {
		"prisma-class-generator": "dist/bin.js"
	},
	"engines": {
		"node": ">=14"
	},
	"author": {
		"name": "kimjbstar",
		"email": "kimjbstar@gmail.com"
	},
	"dependencies": {
		"@prisma/client": "3.1.1",
		"@prisma/generator-helper": "^3.1.1",
		"@prisma/sdk": "^3.1.1",
		"change-case": "^4.1.2"
	},
	"devDependencies": {
		"@types/node": "^16.6.2",
		"prettier": "^2.4.1",
		"prisma": "3.1.1",
		"swagger-ui-express": "^4.1.6",
		"ts-node": "^10.2.1",
		"tsconfig-paths": "^3.11.0",
		"typescript": "^4.3.5"
	},
	"_moduleAliases": {
		"@src": "dist"
	}
}
